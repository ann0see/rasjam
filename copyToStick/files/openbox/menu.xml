<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="Openbox 3">
		<!-- This requires the presence of the 'openbox-menu' package to work -->
		<menu id="root-menu-53263" label="Power options">
			<item label="Reboot now">
				<action name="Execute">
					<execute>sudo /sbin/reboot</execute>
				</action>
			</item>
			<item label="Shutdown now">
				<action name="Execute">
					<execute>sudo /sbin/shutdown -h -P now</execute>
				</action>
			</item>
		</menu>
		<separator/>
		<item label="Audio config">
			<action name="Execute">
				<execute>qjackctl</execute>
			</action>
		</item>
		<item label="Jamulus">
			<action name="Execute">
				<execute>/usr/local/bin/Jamulus</execute>
			</action>
		</item>
		<menu id="root-menu-772731" label="Advanced">
			<item label="Terminal">
				<action name="Execute">
					<execute>xterm</execute>
				</action>
			</item>
			<item label="Restart GUI">
				<action name="Restart"/>
			</item>
			<item label="Exit GUI">
				<action name="Exit"/>
			</item>
		</menu>
	</menu>
</openbox_menu>
